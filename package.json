{
  "name": "web-ui",
  "label": "Inspect WebUI",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "dev": "pnpm generate-api-types & next dev --turbopack & pnpm generate-css-types",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:orphans": "eslint -c .eslintrc.orphans.cjs \"src/**/*.{ts,tsx}\"",
    "knip:orphans": "knip --config knip.config.ts",
    "type-check": "tsc --noEmit",
    "generate-css-types": "tcm src --watch",
    "generate-css-types-once": "tcm src",
    "generate-api-types": "openapi-typescript http://cerbero-pmg.duckdns.org:8088/openapi.json --output src/types/api/api.d.ts --useOptions",
    "test:watch": "jest --watch",
    "test:e2e": "DEBUG_MOCKS=1 playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.2.1",
    "@tanstack/react-table": "^8.21.3",
    "@types/concaveman": "^1.1.6",
    "@types/mapbox-gl": "^3.4.1",
    "@vercel/analytics": "^1.5.0",
    "allotment": "^1.20.4",
    "classnames": "^2.5.1",
    "concaveman": "^2.0.0",
    "exceljs": "^4.4.0",
    "mapbox-gl": "^3.15.0",
    "next": "15.4.5",
    "openapi-typescript": "^7.9.1",
    "radix-ui": "^1.4.3",
    "react": "19.1.0",
    "react-arborist": "^3.4.3",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "use-resize-observer": "^9.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@swc/jest": "^0.2.39",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.17",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.4.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jiti": "^2.6.0",
    "knip": "^5.64.1",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.9.2"
  }
}
